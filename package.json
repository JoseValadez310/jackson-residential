{
  "name": "residential",
  "private": true,
  "version": "1.0.0",
  "homepage": " ",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "archive": "ch5-cli archive -p residential -d dist -o archive",
    "deploy": "ch5-cli deploy -p -H 192.169.1.200 -t mobile archive/JacksonProject.ch5z",
    "deploy:xpanel": "ch5-cli deploy -p -H 0.0.0.0 -t web archive/residential.ch5z",
    "deploy:panel": "ch5-cli deploy -p -H 0.0.0.0 -t touchscreen archive/residential.ch5z --slow-mode",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@crestron/ch5-crcomlib": "^2.9.0",
    "@crestron/ch5-webxpanel": "^2.8.0",
    "animate.css": "^4.1.1",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "eruda": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "sass": "^1.83.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@crestron/ch5-shell-utilities-cli": "^2.9.0",
    "@crestron/ch5-utilities-cli": "^2.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.19.7",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
